{
    "compilerOptions": {
        "noImplicitAny": true,
        "noUnusedParameters": false,
        "noUnusedLocals": true,
        "noImplicitReturns": true,
        "noFallthroughCasesInSwitch": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "removeComments": true,
        "sourceMap": true,
        "target": "es5",
        "module": "commonjs"
    }
}